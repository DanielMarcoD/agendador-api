{
  "name": "agendador-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc -p .",
    "lint": "eslint .",

    "dev:up": "./scripts/dev-up.sh",
    "dev:down": "./scripts/dev-down.sh",

    "db:up": "docker compose up -d db",
    "db:stop": "docker compose stop db",
    "db:restart": "docker compose restart db",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f db",

    "db:reset": "docker compose down -v && docker compose up -d db && ./scripts/wait-for-db.sh && npm run prisma:reset",

    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma generate && prisma migrate deploy",
    "prisma:dev": "prisma generate && prisma migrate dev",
    "prisma:reset": "prisma generate && prisma migrate reset --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielMarcoD/agendador-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielMarcoD/agendador-api/issues"
  },
  "homepage": "https://github.com/DanielMarcoD/agendador-api#readme",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@prisma/client": "^6.16.2",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
